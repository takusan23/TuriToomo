{
	"width": 640,
	"height": 320,
	"fps": 30,
	"main": "./script/_bootstrap.js",
	"assets": {
		"player": {
			"type": "image",
			"width": 32,
			"height": 32,
			"path": "image/player.png"
		},
		"shot": {
			"type": "image",
			"width": 16,
			"height": 16,
			"path": "image/shot.png"
		},
		"se": {
			"type": "audio",
			"path": "audio/se",
			"systemId": "sound",
			"duration": 250
		},
		"main": {
			"type": "script",
			"path": "script/main.js",
			"global": true
		},
		"parameterObject": {
			"type": "script",
			"path": "script/parameterObject.js",
			"global": true
		},
		"_bootstrap": {
			"type": "script",
			"path": "script/_bootstrap.js",
			"global": true
		},
		"toomo": {
			"type": "image",
			"width": 75,
			"height": 75,
			"path": "image/toomo.png"
		},
		"karaoke": {
			"type": "image",
			"width": 100,
			"height": 90,
			"path": "image/karaoke.png"
		}
	},
	"environment": {
		"sandbox-runtime": "2",
		"niconico": {
			"supportedModes": [
				"ranking"
			]
		}
	},
	"globalScripts": [
		"node_modules/@akashic-extension/akashic-label/lib/DefaultRubyParser.js",
		"node_modules/@akashic-extension/akashic-label/lib/FragmentDrawInfo.js",
		"node_modules/@akashic-extension/akashic-label/lib/Label.js",
		"node_modules/@akashic-extension/akashic-label/lib/RubyParser.js",
		"node_modules/@akashic-extension/akashic-label/lib/index.js"
	],
	"moduleMainScripts": {
		"@akashic-extension/akashic-label": "node_modules/@akashic-extension/akashic-label/lib/index.js"
	}
}